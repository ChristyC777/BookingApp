public class ThreadCreation extends Thread 
{
    String name;
    int n;

    ThreadCreation(){}

    ThreadCreation(String threadName)
    {
        super(threadName);
    }

    @Override
    public void run() {
        
        for (int i = 0; i < n; i++) {
            System.out.println("Thread: " + i);

        }
    }

}